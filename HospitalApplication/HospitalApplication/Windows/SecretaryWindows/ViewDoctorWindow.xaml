        <TextBlock Height="23" HorizontalAlignment="Left" Margin="212,318,0,0" Name="textBlockEmail" Text="Email:" VerticalAlignment="Top" Width="110"  />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="212,291,0,0" Name="textBlockPhoneNumber" Text="Phone Number:" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.5,-1.866" />

        <TextBox IsReadOnly="True" Height="23" HorizontalAlignment="Left" Margin="343,181,0,0" Name="textBoxJMBG" VerticalAlignment="Top" Width="222" />
        <TextBox IsReadOnly="True" Height="23" HorizontalAlignment="Left" Margin="343,209,0,0" Name="textBoxSex" VerticalAlignment="Top" Width="222" />
        <TextBox IsReadOnly="True" Height="23" HorizontalAlignment="Left" Margin="343,126,0,0" Name="textBoxFirstName" VerticalAlignment="Top" Width="222"  />
        <TextBox IsReadOnly="True" Height="22" HorizontalAlignment="Left" Margin="343,154,0,0" Name="textBoxLastName" VerticalAlignment="Top" Width="222" />
