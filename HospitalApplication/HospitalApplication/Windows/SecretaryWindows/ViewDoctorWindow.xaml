        <TextBox IsReadOnly="True" Height="22" HorizontalAlignment="Left" Margin="343,292,0,0" Name="textBoxPhoneNumber" VerticalAlignment="Top" Width="222" />
        <TextBox IsReadOnly="True" Height="22" HorizontalAlignment="Left" Margin="343,319,0,0" Name="textBoxEmail" VerticalAlignment="Top" Width="222" />





        <TextBlock Height="23" HorizontalAlignment="Left" Margin="212,98,0,0" Name="textBlockDoctorType" Text="Type Doctor:" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.5,-1.866" />
        <TextBox IsReadOnly="True" Height="23" HorizontalAlignment="Left" Margin="343,98,0,0" Name="ComboBox1" VerticalAlignment="Top" Width="222" RenderTransformOrigin="-0.23,-2.857" />

        <TextBlock Height="24" HorizontalAlignment="Left" Margin="212,345,0,0" Name="textBlockUsername" Text="Username:" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.5,-1.866" />
        <TextBlock Height="22" HorizontalAlignment="Left" Margin="212,375,0,0" Name="textBlockPassword" Text="Password:" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.491,0.736"  />

        <TextBox IsReadOnly="True" Height="23" HorizontalAlignment="Left" Margin="343,346,0,0" Name="textBoxUsername" VerticalAlignment="Top" Width="222" />
        <TextBox IsReadOnly="True" Height="23" HorizontalAlignment="Left" Margin="343,374,0,0" Name="textBoxPassword" VerticalAlignment="Top" Width="222" />

        <Button Content="OK" Height="22" HorizontalAlignment="Left" Margin="364,442,0,0" Name="button2" VerticalAlignment="Top" Width="70" Click="button2_Click" RenderTransformOrigin="1.352,4.222" />


