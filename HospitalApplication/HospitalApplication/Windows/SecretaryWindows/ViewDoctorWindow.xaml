        <TextBlock Height="23" HorizontalAlignment="Left" Margin="212,181,0,0" Name="textBlockJMBG" Text="JMBG:" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.992,0.656" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="212,209,0,0" Name="textBlockSex" Text="Sex:" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.992,0.656" />

