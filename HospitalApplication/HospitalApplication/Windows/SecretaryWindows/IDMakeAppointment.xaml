<Window x:Class="HospitalApplication.Windows.Secretary.IDMakeAppointment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalApplication.Windows.Secretary"
        mc:Ignorable="d"
        Title="IDMakeAppointment" Height="450" Width="800">
    <Grid Margin="10">
        <StackPanel>
            <Label Content="ID Patient"></Label>
            <TextBox Margin="0 0 0 10" Name="IdPatient"></TextBox>
            <Button Content="OK" x:Name="ButtonOk" Click="ButtonOk_Click" ></Button>
        </StackPanel>
    </Grid>
</Window>
