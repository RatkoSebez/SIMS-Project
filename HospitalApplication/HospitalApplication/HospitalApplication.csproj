<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Windows\ManagerWindows\Images\arrow.png" />
    <None Remove="Windows\ManagerWindows\Images\home_icon.jpg" />
    <None Remove="Windows\ManagerWindows\Images\logout.png" />
    <None Remove="Windows\ManagerWindows\Images\minus.png" />
    <None Remove="Windows\ManagerWindows\Images\plus.png" />
    <None Remove="Windows\ManagerWindows\Images\right-arrow.png" />
